<mvc:View
    controllerName="sap.ui.demo.template.controller.Main"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page id="page" title="{i18n>title}">
        <content>
            <List items="{/Products}">
                <StandardListItem type="Active" title="{ProductName}"/>
            </List>
        </content>
    </Page>
</mvc:View>